# ─── Supabase ───
POSTGRES_PASSWORD=your-postgres-password
JWT_SECRET=your-jwt-secret-min-32-chars
ANON_KEY=your-supabase-anon-key
SERVICE_ROLE_KEY=your-supabase-service-role-key
SITE_URL=https://your-app-domain.com
API_EXTERNAL_URL=https://your-supabase-domain.com

# ─── Supabase Auth (Google OAuth) ───
GOTRUE_EXTERNAL_GOOGLE_ENABLED=true
GOTRUE_EXTERNAL_GOOGLE_CLIENT_ID=your-google-client-id
GOTRUE_EXTERNAL_GOOGLE_SECRET=your-google-client-secret
GOTRUE_EXTERNAL_GOOGLE_REDIRECT_URI=https://your-supabase-domain.com/auth/v1/callback
GOTRUE_EXTERNAL_GOOGLE_SKIP_NONCE_CHECK=true

# ─── Supabase SMTP ───
SMTP_HOST=smtp.your-provider.com
SMTP_PORT=465
SMTP_USER=your-email
SMTP_PASS=your-password
SMTP_ADMIN_EMAIL=admin@yourdomain.com

# ─── LiveKit ───
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret
LIVEKIT_URL=wss://livekit.yourdomain.com

# ─── AI Services ───
OPENAI_API_KEY=your-openai-api-key
DEEPGRAM_API_KEY=your-deepgram-api-key
